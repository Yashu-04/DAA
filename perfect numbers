#include<stdio.h>
int main()
{
   int count=0;
   int number,i,result=0;
   count++;
   printf("enter the number:");
   count++;
   scanf("%d",&number);
   count++;
   for(i=1;i<=number;i++)
   {
		count++;
		count++;
		count++;   	
      if(number%i==0)
      count++;
         result=result+i;
         count++;
   }
   if(result==2*number)
   {
   	count++;
    printf("perfect number");
	}
   else
   {
   	count++;
      printf("not perfect number");
	}
	printf("\n time complexity:%d",count);
}
